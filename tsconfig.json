{
    "compilerOptions": {
      "jsx": "react",
      "moduleResolution": "node",
      "outDir": "dist",
      "target": "es5",
      "lib": ["es6", "dom"],
      "module": "esnext",
      "declaration": false,
      "diagnostics": true,
      "allowSyntheticDefaultImports": true,
      "allowJs": true,
      "sourceMap": true,
      "sourceRoot": "src",
      "inlineSources": true,
      "noEmitOnError": false,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "noImplicitReturns": true,
      "noImplicitThis": false,
      "noImplicitUseStrict":false,
      "noImplicitAny": false,
      "noUnusedLocals": false,
      "baseUrl": "./src"
    }
}
